{{extend 'layout.html'}}
<br><br>
<div class="searchpage">
<form id="search_form">
    <input name="query"/>
    <select name="option">
        <option value="Tags">Tags</option>
        <option value="Uploader">Uploader</option>
        <option value="Title">Title</option>
    </select>
    <input type="button" value="Submit Query" id="Submit"/>
</form>
</div>
<div id="tar-for">
</div>

<script>
    jQuery('#Submit').click(
    function(){
        ajax('{{=URL('search_list')}}', ['query','option'],'tar-for');
    });
</script>
